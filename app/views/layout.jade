doctype html
html.spread(lang='en')
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    block title

    link(rel='stylesheet' href='//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css')
    link(rel='stylesheet' href="//code.jquery.com/ui/1.10.4/themes/black-tie/jquery-ui.css")
    link(rel='stylesheet' href='/styles/out.min.css')
    link(rel='stylesheet' type='text/css' href='//fonts.googleapis.com/css?family=Open Sans:300')
    block styles
    include includes/analytics

  body.spread
    block content

    script(src='//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js')
    script(src='//netdna.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js')
    block scripts
